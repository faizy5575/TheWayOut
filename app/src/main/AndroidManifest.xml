<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exmple.taimoor.thewayout">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".SliderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SportsActivity" />
        <activity android:name=".HomeServices" />
        <activity android:name=".DealsActivity" />
        <activity android:name=".ActivitiesActivity" />
        <activity android:name=".ReservationActivity" />
        <activity android:name=".DirectoryActivity" />
        <activity android:name=".FacilitiesActivity" />
        <activity android:name=".Registration" />
        <activity android:name=".Electrician"></activity>
    </application>

</manifest>